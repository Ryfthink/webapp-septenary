{
  "name": "septenary",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start-aot": "concurrently --kill-others \"ng serve --aot=true --proxy-config proxy.conf.json\" \"npm run server-run\"",
    "start": "concurrently --kill-others \"ng serve --proxy-config proxy.conf.json\" \"npm run server-run\"",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "server-debug": "DEBUG=septenary:* node ./server/bin/www",
    "server-build": "tsc -p ./server && cp -R ./server/public ./dist/server",
    "server-run": "npm run server-build && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "build": "ng build --prod --sm=false --aot=true --output-path=dist/client && npm run server-build"
  },
  "author": "RenYufeng",
  "repository": {
    "url": "https://github.com/ryfthink"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.20",
    "@types/body-parser": "0.0.33",
    "@types/compression": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/node-uuid": "0.0.28",
    "body-parser": "^1.16.0",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "node-uuid": "^1.4.7",
    "rxjs": "^5.0.1",
    "serve-favicon": "~2.3.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.24",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "nodemon": "^1.11.0",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.0.2",
    "typescript": "~2.0.3"
  }
}
